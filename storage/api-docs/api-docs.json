{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel 12 Simple API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api",
            "description": "Local API Server"
        }
    ],
    "paths": {
        "/getAllProduct": {
            "get": {
                "tags": [
                    "Products"
                ],
                "operationId": "f499ddd1721970aceed69657398d0db8",
                "responses": {
                    "200": {
                        "description": "Get All Products"
                    }
                }
            }
        },
        "/getProductById/{id}": {
            "get": {
                "tags": [
                    "Products"
                ],
                "operationId": "20643b835b55432e1f96fc991185d9f6",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the product to fetch",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 3
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get Product By Id"
                    }
                }
            }
        },
        "/store": {
            "post": {
                "tags": [
                    "Products"
                ],
                "summary": "Create a product by ID",
                "description": "Store a newly created resource in storage.",
                "operationId": "99ca993cdd9d55003d55dfb70c88a96b",
                "parameters": [
                    {
                        "name": "X-CSRF-TOKEN",
                        "in": "header",
                        "description": "CSRF token required for store requests",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "your-csrf-token-here"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "price": {
                                        "type": "number"
                                    },
                                    "category_id": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Product Created successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Product not found"
                    }
                }
            }
        },
        "/update/{id}": {
            "put": {
                "tags": [
                    "Products"
                ],
                "summary": "Update a product by ID",
                "description": "Update the specified resource in storage.",
                "operationId": "dd19fb5746fb095bbb552dcbb00afff8",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 6
                    },
                    {
                        "name": "X-CSRF-TOKEN",
                        "in": "header",
                        "description": "CSRF token required for Update requests",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "your-csrf-token-here"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "price": {
                                        "type": "number"
                                    },
                                    "category_id": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Updated successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Product Updated successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Product not found"
                    }
                }
            }
        },
        "/delete/{id}": {
            "delete": {
                "tags": [
                    "Products"
                ],
                "summary": "Delete a product by ID",
                "description": "Remove the specified resource from storage.",
                "operationId": "066fb076f6f5a25c7161278aacd5e89d",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 6
                    },
                    {
                        "name": "X-CSRF-TOKEN",
                        "in": "header",
                        "description": "CSRF token required for DELETE requests",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "your-csrf-token-here"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Deleted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Product deleted successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Product not found"
                    }
                }
            }
        },
        "/csrf-token": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Get CSRF token",
                "operationId": "db80c73342c0abb67e50761a11c6f96a",
                "responses": {
                    "200": {
                        "description": "Returns CSRF token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "csrf_token": {
                                            "type": "string",
                                            "example": "somerandomtokenhere"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Products",
            "description": "Products"
        },
        {
            "name": "Auth",
            "description": "Auth"
        }
    ]
}